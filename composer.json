{
    "name": "laraxot/module_xot",
    "description": ":package_description",
    "keywords": [
        "laraxot",
        "laravel",
        "module_xot"
    ],
    "homepage": "https://github.com/laraxot/module_xot",
    "license": "MIT",
    "authors": [{
        "name": "marco sottana",
        "email": "marco.sottana@gmail.com",
        "role": "Developer"
    }],
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "*",
        "fidum/laravel-eloquent-morph-to-one": "^2.2",
        "guzzlehttp/guzzle": "^7.6",
        "illuminate/contracts": "^10.0",
        "laravelcollective/html": "^6.4",
        "livewire/livewire": "^2.12",
        "nwidart/laravel-modules": "^9",
        "spatie/laravel-data": "^3.5",
        "spatie/laravel-model-status": "^1.11",
        "spatie/laravel-package-tools": "^1.14.0",
        "spatie/laravel-permission": "^5.10",
        "spatie/laravel-queueable-action": "^2.14",
        "spatie/laravel-tags": "^4.3"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Modules\\Xot\\": ""
        }
    },
    "scripts": {

        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\Xot\\Provider\\XotServiceProvider"

            ],
            "aliases": {}
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
